{
    "name": "webong/guzzle-telescope-middleware",
    "description": "A Telescope middleware for guzzle http >= 6.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Wisdom Ebong",
            "email": "wisdomaebong@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Webong\\GuzzleTelescopeMiddleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webong\\GuzzleTelescopeMiddleware\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
